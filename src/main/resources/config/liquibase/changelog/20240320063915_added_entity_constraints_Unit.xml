<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-latest.xsd">
    <!--
        Added the constraints for entity Unit.
    -->
    <changeSet id="20240320063915-2" author="jhipster">

        <addForeignKeyConstraint baseColumnNames="emp_service_id"
                                 baseTableName="unit"
                                 constraintName="fk_unit__emp_service_id"
                                 referencedColumnNames="id"
                                 referencedTableName="emp_service"
                                 />

        <addForeignKeyConstraint baseColumnNames="user_id"
                                 baseTableName="unit"
                                 constraintName="fk_unit__user_id"
                                 referencedColumnNames="id"
                                 referencedTableName="jhi_user"
                                 />
    </changeSet>
</databaseChangeLog>
